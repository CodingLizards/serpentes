{
    "appenders": [
        {
            "type": "logLevelFilter",
            "level": "WARN",
            "appender": {
                "type": "file",
                "filename": "log/serpentes.log",
                "alwaysIncludePattern": true
            }
        },
        {
            "type": "logLevelFilter",
            "level": "DEBUG",
            "appender": {
                "type": "console"
            }
        }
    ],
    "replaceConsole": true
}
